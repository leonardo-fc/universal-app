{
  "name": "universal-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start $(tsx scripts/tunnel.ts)",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "start:web": "expo start --web",
    "start:desktop": "tauri dev",
    "build:web": "expo export:web",
    "build:desktop": "tauri build",
    "get-story": "sb-rn-get-stories --config-path .storybook_ondevice",
    "story": "pnpm get-story && tsx scripts/startStory.ts",
    "check": "tsx scripts/check.ts",
    "lint": "eslint \"./**/*.{js,ts,tsx}\"",
    "fmt-check": "prettier --check .",
    "spell-check": "cspell --no-progress",
    "type-check": "tsc --noEmit",
    "test": "NODE_ENV=test pnpm get-story && jest",
    "test:dev": "pnpm test --watchAll",
    "test:generate-output": "echo {} > tests/.results.json && NODE_ENV=test pnpm get-story && jest --json --outputFile=tests/.results.json",
    "postinstall": "tsx scripts/shim.ts",
    "postuninstall": "tsx scripts/shim.ts"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo-*|@expo|@storybook/react-native|@miblanchard/react-native-slider/)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@miblanchard/react-native-slider": "^2.3.0",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "date-fns": "^2.29.3",
    "expo": "~48.0.6",
    "expo-asset": "~8.9.1",
    "expo-av": "~13.2.1",
    "expo-constants": "~14.2.1",
    "expo-font": "~11.1.1",
    "expo-linking": "~4.0.1",
    "expo-navigation-bar": "~2.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.8",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-web": "~0.18.12"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@expo/ngrok": "^4.1.0",
    "@expo/webpack-config": "^18.0.1",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/datetimepicker": "6.7.3",
    "@react-native-community/slider": "^4.4.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-jest": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react-native": "^6.5.3",
    "@storybook/react-native-server": "^6.5.3",
    "@tauri-apps/cli": "^1.2.3",
    "@testing-library/react-native": "^11.5.4",
    "@types/jest": "^29.4.0",
    "@types/react": "~18.0.28",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "cspell": "^6.27.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-expo": "~48.0.2",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.2.7",
    "tsx": "^3.12.3",
    "typescript": "^4.6.3",
    "url": "^0.11.0",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1"
  },
  "packageManager": "pnpm@7.29.0",
  "engines": {
    "node": ">=16",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=7"
  },
  "private": true
}
